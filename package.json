{
  "name": "async-fs",
  "version": "1.0.0",
  "description": "Async wrappers for node's filesystem module",
  "main": "dist/main/index.js",
  "repository": "git@github.com:Recombix/async-fs.git",
  "author": "Bill Beesley <bill@recombix.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "BABEL_ENV=production babel src --out-dir dist --copy-files --source-maps --extensions \".js\" --ignore \"src/test/**\"",
    "postcompile": "./node_modules/.bin/documentation readme src/main/index.js -s \"api\" --github",
    "lint": "./node_modules/.bin/eslint src/**  --fix",
    "mocha": "BABEL_ENV=test nyc mocha --require ./src/test/index.js 'src/test/**/*.test.js'",
    "pretest": "run-p clean lint",
    "posttest": "yarn compile",
    "test": "BABEL_ENV=test #yarn mocha"
  },
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-template": "^6.26.0",
    "babel-types": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.0.5",
    "mocha-sinon": "^2.0.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "documentation": "^8.0.0"
  }
}
